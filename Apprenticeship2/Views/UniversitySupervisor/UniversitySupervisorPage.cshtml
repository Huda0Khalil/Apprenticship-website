@{
}
@using Apprenticeship2.Data.Entities

<a href="/UniversitySupervisor/Add" class="btn btn-primary mb-4">create new Supervisor</a>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h2 class="card-title text-center">University Supervisors</h2>
                <div class="table-responsive">
                    <table id="myTable" class="table table-striped table-bordered zero-configuration">
  <thead>
        <tr>
            <th>firtName</th>
            <th>secondName</th>
            <th>thirdName</th>
            <th>lastName</th>
            <th>email</th>
            <th>University</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
  </thead>
   <tbody>
        @foreach (var s in (List<UniversitySupervisor>)ViewBag.sup)
        {
            <tr>
                <td>@s.FirstName</td>
                <td>@s.SecondName</td>
                <td>@s.ThirdName</td>
                <td>@s.LastName</td>
                <td>@s.Email</td>
                <td>@s.university.universityName</td>
                <td><a class="icon-note menu-icon text-warning" href="/UniversitySupervisor/Update?sup=@s.Id"></a></td>
                <td><a class="ti-trash text-danger" href="/UniversitySupervisor/delete?sup=@s.Id"></a></td>
            </tr>
        }
   </tbody>
  
</table>
</div>
</div>
</div>
</div>
</div>